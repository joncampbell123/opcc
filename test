opcode "FXCH"
  (comment "FXCH st(i) and st(0)")
  (reads st(rm),st(0))
  (writes st(rm),st(0))
  (dest=st(0))
  (param=st(rm))
  (code 0xD9 mrm mod(3) reg(1) fpu);

opcode "FCOM"
  (comment "FCOM MF=0 [esc]11011 [mf]00 [esc]0 float32 from memory")
