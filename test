    (code 0x66 0x0F 0x73 mrm i=immediate(b) reg(7) sse2);

  opcode "PSRLDQ"
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=i)
    (code 0x66 0x0F 0x73 mrm i=immediate(b) reg(3) sse2);

  opcode "PUNPCKHQDQ"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0x66 0x0F 0x6D mrm sse2);

  opcode "PUNPCKLQDQ"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0x66 0x0F 0x6c mrm sse2);
} if;

if ((value("sse3") > 0) or (value("everything") > 0)) {;
  opcode "MONITOR"
    (comment "0x0F 0x01 0xC8")
    (reads eax, ecx, edx)
    (param(0)=eax)
    (param(1)=ecx)
    (param(2)=edx)
    (code 0x0F 0x01 mrm mod(3) reg(1) rm(0));

  opcode "MWAIT"
    (comment "0x0F 0x01 0xC9")
    (reads eax, ecx)
    (param(0)=eax)
    (param(1)=ecx)
    (code 0x0F 0x01 mrm mod(3) reg(1) rm(1));

  opcode "FISTTP"
    (reads st(0))
    (writes rm(sw))
    (dest=rm(sw))
    (param=st(0))
    (code 0xDF mrm mod(!3) reg(1) fpu);

  opcode "FISTTP"
    (reads st(0))
    (writes rm(sdw))
    (dest=rm(sdw))
    (param=st(0))
    (code 0xDB mrm mod(!3) reg(1) fpu);

  opcode "FISTTP"
    (reads st(0))
    (writes rm(sqw))
    (dest=rm(sqw))
    (param=st(0))
    (code 0xDD mrm mod(!3) reg(1) fpu);

  opcode "ADDSUBPS"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0xF2 0x0F 0xD0 mrm);

  opcode "ADDSUBPD"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0x66 0x0F 0xD0 mrm);

  opcode "MOVDDUP"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0xF2 0x0F 0x12 mrm);

  opcode "MOVSLDUP"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0xF3 0x0F 0x12 mrm);

  opcode "MOVSHDUP"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0xF3 0x0F 0x16 mrm);

  opcode "HADDPS"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0xF2 0x0F 0x7C mrm);

  opcode "HADDPD"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0x66 0x0F 0x7C mrm);

  opcode "HSUBPS"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0xF2 0x0F 0x7D mrm);

  opcode "HSUBPD"
    (reads xmm(rm))
    (writes xmm(reg))
    (dest=xmm(reg))
    (param=xmm(rm))
    (code 0x66 0x0F 0x7D mrm);
} if;
