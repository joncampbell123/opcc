  (fpu stack push rm(f64))
  (dest=st(0))
  (param=rm(f64))
  (code 0xDD mrm mod(!3) reg(0) fpu);

opcode "FILD"
  (comment "FLD MF=1 [esc]11011 [mf]11 [esc]1 int16 from memory")
