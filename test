  (param=st(0))
  (dest=rm(sw))
  (code 0xDF mrm mod(!3) reg(3) fpu);

opcode "FSTP"
  (comment "FLD [esc]11011101 st(0) to st(i)")
  (fpu stack pop st(rm))
  (reads fpu(top),st(0))
  (writes fpu(top),st(rm))
  (modifies fpu(top))
  (param=st(0))
  (dest=st(rm))
  (code 0xDD mrm mod(3) reg(3) fpu);

