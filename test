  opcode "PMVZB"
    (comment "i=implied register")
    (reads mm(reg),mm(rm),mm(implied))
    (modifies mm(reg))
    (writes mm(reg))
    (dest=mm(reg))
    (param(0)=mm(rm))
    (param(1)=mm(implied))
    (code 0x0F 0x58 mrm emmi);

  opcode "PSUBSIW"
    (comment "i=implied register")
    (reads mm(reg),mm(rm))
    (modifies mm(implied))
    (writes mm(implied))
    (dest=mm(implied))
    (param(0)=mm(reg))
    (param(1)=mm(rm))
    (code 0x0F 0x55 mrm emmi);
} if;

if value("sse") > 0 {;
  opcode "MASKMOVQ"
    (comment "writes to ds:[div], mm(reg)=value mm(rm)=byte mask")
    (reads mm(reg),mm(rm),div,seg)
    (modifies far memory(qw,seg,div))
    (writes far memory(qw,seg,div))
    (dest=far memory(qw,seg,div))
    (param(0)=mm(reg))
    (param(1)=mm(rm))
